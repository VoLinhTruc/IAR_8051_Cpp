P1 EQU 90H
P1DIR EQU 0FEH
P1SEL EQU 0F4H

P0 EQU 80H
P0DIR EQU 0FDH
P0SEL EQU 0F3H

P0IFG EQU 89H
P0IEN EQU 0ABH
IEN1 EQU 0B8H
IEN0 EQU 0A8H
PICTL EQU 0x8C

VAL EQU 20H
  
  ORG 00000H
  MOV VAL, #55H
  LJMP main
  
  ORG 0006BH
    LJMP LOOP1
  RETI
  
  main:
  ORG 00094H
  MOV P0SEL, #00H
  MOV P0DIR, #00H
  
  MOV P0IFG, #00H
  MOV P0IEN, #0FFH
  MOV IEN1, #20H
  MOV IEN0, #80H
  MOV PICTL, #0FH
  
  LOOP:
  MOV P1SEL, #00H
  MOV P1DIR, #0FFH
  MOV P1, VAL
  JMP LOOP
  
  LOOP1:
    INC VAL
   RETI
  END